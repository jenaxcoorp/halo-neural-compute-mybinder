#!/usr/bin/env bash
set -euo pipefail
python - <<'PY'
from fastapi import FastAPI
import uvicorn
app = FastAPI()
@app.get("/")
def root():
    return {"status": "ok", "message": "Agent Zero UI placeholder via FastAPI on Binder", "hint": "This proves /proxy/50080/ works."}
if __name__ == "__main__":
    uvicorn.run(app, host="0.0.0.0", port=50080)
PY
